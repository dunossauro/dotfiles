#!/usr/bin/env bash

python-upgrade(){

    echo "update pyenv"
    pyenv update

    echo "upgrade pythons"
    pyenv install 3.10:latest -s
    pyenv install 3.11:latest -s
    pyenv install 3.12:latest -s

    echo "Upgrade pipx package"
    # pipx reinstall-all --python python3.11
    pipx upgrade-all
}
